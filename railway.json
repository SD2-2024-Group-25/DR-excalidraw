{
  "build": [
    {
      "service": "excalidraw",
      "command": "cd excalidraw && yarn install && yarn run build",
      "outputDirectory": "excalidraw/dist"
    },
    {
      "service": "excalidraw-room",
      "command": "cd excalidraw-room && yarn install && yarn run build",
      "outputDirectory": "excalidraw-room/dist"
    },
    {
      "service": "excalidraw-storage-backend",
      "command": "cd excalidraw-storage-backend && npm install && npm run prebuild && npm run build",
      "outputDirectory": ""
    }
  ],
  "start": [
    {
      "service": "excalidraw",
      "command": "cd excalidraw && yarn start"
    },
    {
      "service": "excalidraw-room",
      "command": "cd excalidraw-room && yarn start"
    },
    {
      "service": "excalidraw-storage-backend",
      "command": "cd excalidraw-storage-backend && npm start"
    }
  ]
}
